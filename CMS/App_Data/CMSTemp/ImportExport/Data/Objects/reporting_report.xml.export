GROUP BY OM_Contact.ContactID, ContactFirstName, ContactLastName, ContactEmail, ContactStatusName
HAVING SUM(OM_ScoreContactRule.Value) &gt;= @MinimalScore
ORDER BY [{$om.score$}] DESC</TableQuery>
