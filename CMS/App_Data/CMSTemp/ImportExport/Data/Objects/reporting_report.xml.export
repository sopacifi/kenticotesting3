   AND ((StatusSiteID = @CMSContextCurrentSiteID AND @GlobalObjects = 0) 
         OR (StatusSiteID IS NULL AND @GlobalObjects = 1)) 
GROUP BY 
   StatusDisplayName, 
   StatusOrder
ORDER BY StatusOrder</GraphQuery>
