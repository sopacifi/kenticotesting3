    AND (@DateFrom IS NULL OR (OrderDate &gt;= @DateFrom))
    AND (@DateTo IS NULL OR (OrderDate &lt;= DATEADD(day,1,@DateTo)))
    AND (ISNULL(OrderIsPaid, 0) = 1 OR ISNULL(@IsPaid,0) = 0)
    AND OrderSiteID = @CMSContextCurrentSiteID)
