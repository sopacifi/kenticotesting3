DECLARE @GlobalProducts bit;
SET @GlobalProducts = {%Settings.CMSStoreAllowGlobalProducts?1:0|(identity)GlobalAdministrator|(hash)e438dc2a644bbd7997189483ddb01d98189c78a37231856423961dc6d8f08e55%};
DECLARE @GlobalOptions bit;
