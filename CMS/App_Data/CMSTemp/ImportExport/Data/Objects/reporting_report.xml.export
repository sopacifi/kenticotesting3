    WHERE ([InsightCodeName] = @CodeName) AND ([InsightPeriodType] = @PeriodType)
      AND ([InsightExternalID] = @ExternalID) AND ([InsightHitPeriodFrom] BETWEEN @FromDate AND @ToDate)
  )
