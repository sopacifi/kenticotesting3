   AND ((StatusSiteID = @CMSContextCurrentSiteID AND @GlobalStatus = 0)  
        OR (StatusSiteID IS NULL AND @GlobalStatus = 1))
GROUP BY 
   StatusDisplayName, 
   StatusOrder 
ORDER BY StatusOrder</GraphQuery>
