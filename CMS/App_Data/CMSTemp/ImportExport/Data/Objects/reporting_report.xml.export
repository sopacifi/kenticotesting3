FROM 
   COM_OrderStatus 
   LEFT JOIN COM_Order 
      ON OrderStatusID = StatusID 
      AND OrderSiteID = @CMSContextCurrentSiteID 
      AND (OrderDate &gt;= @FromDate OR @FromDate IS NULL) 
      AND (OrderDate &lt;= DATEADD(day,1,@ToDate) OR @ToDate IS NULL)
