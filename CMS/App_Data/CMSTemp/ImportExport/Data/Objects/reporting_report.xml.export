  WHERE (StatisticsSiteID = @CMSContextCurrentSiteID)
  AND (StatisticsCode=@CodeName)
  AND (HitsStartTime &gt;= @FromDate) AND (HitsEndTime &lt;= @ToDate)
  GROUP BY StatisticsObjectName
